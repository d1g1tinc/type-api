{
  "extends": ["tslint-microsoft-contrib", "tslint-react", "tslint-eslint-rules"],
  "rulesDirectory": ["tslint-consistent-codestyle", "tslint-microsoft-contrib"],
  "rules": {
    /**
     * Overrides of "tslint-microsoft-contrib" and "tslint-react".
     */
    "eofline": true,
    "indent": [
      true,
      "spaces"
    ],
    "import-name": false,
    "ordered-imports": [false, {}],
    "no-console": [
      false,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace",
      "log"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-single"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "no-var-requires": false,
    "member-access": false,
    "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
    "max-classes-per-file": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "jsx-no-multiline-js": false,
    "max-line-length": [false],
    "no-empty-interface": false,
    "newline-before-return": true,
    "missing-jsdoc": false,
    "no-http-string": [false],
    "object-literal-key-quotes": [false],
    "typedef": false,
    "react-tsx-curly-spacing": [true, "never", {"allowMultiline": true}],
    "no-backbone-get-set-outside-model": false,
    "whitespace": true,
    "object-curly-spacing": [
      true,
      "never"
    ],
    "export-name": false,
    "no-reserved-keywords": false,
    "no-any": false,
    "no-relative-imports": false,
    "no-invalid-this": false,
    "no-import-side-effect": false,
    "no-arg": false,
    "array-type": false,
    "prefer-array-literal": false,
    "no-shadowed-variable": false,
    /**
     * Additional rules to improve code quality
     */
    "cyclomatic-complexity": true,
    "no-else-after-return": true,
    "no-var-before-return": true,
    "early-exit": true,
    "no-collapsible-if": true,
    "no-return-undefined": true,
    "no-static-this": true,
    "prefer-while": true,
    /**
     * Accessibility. The following rules should be turned on to guarantee the best user
     * experience for keyboard and screen reader users.
     * Disabled as no need at this momment
     */
    "react-a11y-anchors": false,
    "react-a11y-aria-unsupported-elements": false,
    "react-a11y-event-has-role": false,
    "react-a11y-image-button-has-alt": false,
    "react-a11y-img-has-alt": false,
    "react-a11y-lang": false,
    "react-a11y-meta": false,
    "react-a11y-props": false,
    "react-a11y-proptypes": false,
    "react-a11y-role": false,
    "react-a11y-role-has-required-aria-props": false,
    "react-a11y-role-supports-aria-props": false,
    "react-a11y-tabindex-no-positive": false,
    "react-a11y-titles": false,
    /**
     * Type Check. Disabled due to type check in tslint. Very slows down build type
     */
    "no-cookies": false,
    "await-promise": false,
    "match-default-export-name": false,
    "no-floating-promises": false,
    "no-for-in-array": false,
    "no-unsafe-any": false,
    "promise-function-async": false,
    "restrict-plus-operands": false,
    "strict-boolean-expressions": false,
    "completed-docs": false,
    "no-unnecessary-qualifier": false,
    "no-void-expression": false,
    "no-use-before-declare": false
  }
}
